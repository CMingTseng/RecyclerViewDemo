<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tellh.recyclerviewdemo">

    <application
        android:name=".AndroidApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.DiffUtilActivity"
            android:label="@string/app_name"
            android:theme="@style/DiffActivityTheme">

        </activity>
        <activity android:name=".activity.StaggeredActivity" />
        <activity
            android:name=".activity.StickyHeaderActivity"
            android:theme="@style/StickyActivityTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>